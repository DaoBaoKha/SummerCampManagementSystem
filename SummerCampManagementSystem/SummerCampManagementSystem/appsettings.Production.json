{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Hangfire": "Information"
        }
    },
    "AIServiceSettings": {
        "Timeout": 60,
        "HealthCheckEndpoint": "/health"
    },
    "Hangfire": {
        "DashboardEnabled": true,
        "RequireAuthentication": false,
        "AllowedRoles": [
            "Admin"
        ]
    }
}